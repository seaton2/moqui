			<warehouse.SdAttribute attributeId="m" attributeTypeId="SIZE" attributeCode="M" attributeDescription="Medium" isCombo="F"/>
			<warehouse.SdAttribute attributeId="l" attributeTypeId="SIZE" attributeCode="L" attributeDescription="Large" isCombo="F"/>
			<warehouse.SdAttribute attributeId="Med" attributeTypeId="WIDTH" attributeCode="MED" attributeDescription="Medium" isCombo="F"/>
			<warehouse.SdAttribute attributeId="Wid" attributeTypeId="WIDTH" attributeCode="WIDE" attributeDescription="Wide" isCombo="F"/>
			<warehouse.SdAttribute attributeId="Nar" attributeTypeId="WIDTH" attributeCode="NAR" attributeDescription="Narrow" isCombo="F"/>
